@model IEnumerable<Api1.Models.Dzivoklis>
@using Api1.Models

@{
    ViewData["Title"] = "HomeView";
}

<h1>HomeView</h1>

<table class="table">
    <tr>
        <th>Film ID</th>
        <th>Title</th>
        <th>Description</th>
        <th>Release Year</th>
        <th>Length</th>
        <th>Rating</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Numurs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stavs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Istabu_Skaits)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Iedzivotaju_Skaits)
            </td>
            @* <td> *@
            @*     @Html.DisplayFor(modelItem => item.Length) *@
            @* </td> *@
            @* <td> *@
            @*     @Html.DisplayFor(modelItem => item.Rating) *@
            @* </td> *@
        </tr>
    }
</table>